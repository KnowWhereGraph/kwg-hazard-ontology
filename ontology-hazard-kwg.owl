        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:58:15Z</dc:date>
        <dc:description>Exposure to a parasite–an organism living on or in a host–causes an unusual increase in the number of incidents. Exposure to parasites occurs mostly through contaminated water, food or contact with insects, animals (zoonotic), pets, etc. Examples are malaria, chagas disease, giardiasis and trichinellosis.</dc:description>
        <rdfs:label xml:lang="en">Parasitic Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000057 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000057">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000146"/>
                <owl:someValuesFrom rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000185"/>
            </owl:Restriction>
        </rdfs:subClassOf>
