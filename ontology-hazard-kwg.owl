    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000260 -->

    <owl:NamedIndividual rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000260">
        <rdf:type rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000250"/>
        <dcterms:description>A mass of snow, sometimes containing rocks, ice, trees, or other debris, that moves rapidly down a steep slope, resulting in a fatality, injury, or significant damage. If a search team inadvertently starts another avalanche, it will be entered as a new Avalanche event.</dcterms:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Avalanche Event Type Instance (NOAA)</rdfs:label>
    </owl:NamedIndividual>
