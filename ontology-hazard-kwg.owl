        <owl:annotatedTarget>Human encounters with dangerous or exotic animals in both urban and rural environments.</owl:annotatedTarget>
        <dcterms:source rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000091"/>
    </owl:Axiom>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000023 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000023">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000007"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000083"/>
