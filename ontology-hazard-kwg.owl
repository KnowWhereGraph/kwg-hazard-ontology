            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:resource="http://orcid.org/0000-0003-0632-7576"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:46:50Z</dc:date>
        <dc:description>The pervasive influx, swarming and/or hatching of insects affecting humans, animals, crops, and perishable goods. Examples are locusts and African Bees.</dc:description>
        <rdfs:label xml:lang="en">Insect Infestation Event</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000025 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000025">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000007"/>
