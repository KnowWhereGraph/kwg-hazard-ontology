        <dc:description>The temporary or permanent loss of sediments or landmass in coastal margins due to the action of waves, winds, tides, or anthropogenic activities.</dc:description>
        <rdfs:label xml:lang="en">Coastal Erosion</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000033 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000033">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000027"/>
