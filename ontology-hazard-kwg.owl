        <dc:description>Surface displacement of earthen materials due to ground shaking triggered by earthquakes or volcanic eruptions.</dc:description>
        <rdfs:label xml:lang="en">Ground Movement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000047 -->

