        <rdfs:label xml:lang="en">Collision</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000036 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000036">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000027"/>
