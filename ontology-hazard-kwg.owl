            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:resource="http://orcid.org/0000-0003-0632-7576"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:55:16Z</dc:date>
        <dc:description>Heavy or excessive rainfall in a short period of time that produce immediate runoff, creating flooding conditions within minutes or a few hours during or after the rainfall.</dc:description>
        <rdfs:label xml:lang="en">Flash Flood</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000042 -->

