    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000279 -->

    <owl:NamedIndividual rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000279">
        <rdf:type rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000250"/>
        <dcterms:description>Sleet accumulations meeting or exceeding locally/regionally defined warning criteria (typical value is ½ inch or more). The Storm Data preparer should include in the narrative the times that sleet accumulation began, met criteria, and ended.</dcterms:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sleet Event Type Instance (NOAA)</rdfs:label>
    </owl:NamedIndividual>
