        <rdfs:label xml:lang="en">Liquefaction</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000056 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000056">
