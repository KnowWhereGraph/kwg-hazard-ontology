        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:53:24Z</dc:date>
        <dc:description>Higher-than-normal water levels along the coast caused by tidal changes or thunderstorms that result in flooding, which can last from days to weeks.</dc:description>
        <rdfs:label xml:lang="en">Coastal Flood</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000034 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000034">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000150"/>
                <owl:someValuesFrom rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000180"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:resource="http://orcid.org/0000-0003-0632-7576"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:53:37Z</dc:date>
        <dc:description>A period of abnormally cold weather. Typically a cold wave lasts two or more days and may be aggravated by high winds. The exact temperature criteria for what constitutes a cold wave vary by location.</dc:description>
        <rdfs:label xml:lang="en">Cold Wave</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000035 -->

