        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:46:44Z</dc:date>
        <dc:description>Either an unusual, often sudden, increase in the number of incidents of an infectious disease that already existed in the region (e.g., flu, E. coli ) or the appearance of an infectious disease previously absent from the region (e.g., plague, polio).</dc:description>
        <rdfs:label xml:lang="en">Disease Event</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000024 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000024">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000007"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000083"/>
