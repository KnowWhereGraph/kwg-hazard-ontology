        <dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:46:39Z</dcterms:date>
        <dcterms:description>Human encounters with dangerous or exotic animals in both urban and rural environments.</dcterms:description>
        <rdfs:label xml:lang="en">Animal Incident Event</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000022"/>
