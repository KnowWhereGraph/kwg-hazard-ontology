        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2021-04-24T00:54:05Z</dc:date>
        <dc:description>Types of landslides that occur when heavy rain or rapid snow/ice melt send large amounts of vegetation, mud, or rock downslope by gravitational forces.</dc:description>
        <rdfs:label xml:lang="en">Debris/Mud Flow/Rockfall</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.dataone.org/odo/KWG_Hazards_00000037 -->

    <owl:Class rdf:about="http://purl.dataone.org/odo/KWG_Hazards_00000037">
        <rdfs:subClassOf rdf:resource="http://purl.dataone.org/odo/KWG_Hazards_00000027"/>
